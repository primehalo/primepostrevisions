{
	"name": "primehalo/primepostrevisions",
	"type": "phpbb-extension",
	"description": "Keeps a revision history of all posts edits. These revisions can be viewed, deleted, and restored only by users with the necessary permissions.",
	"homepage": "http://www.absoluteanime.com/_phpbb-extensions_/",
	"version": "1.0.0-beta1",
	"time": "2018-05-02",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Ken F. Innes IV",
			"email": "primehalo@gmail.com",
			"homepage": "http://www.absoluteanime.com/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Prime Post Revisions",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3,<3.3.0@dev"
		}
	},
	"version-check": {
	    "host": "www.absoluteanime.com",
		"directory": "/_phpbb-extensions_/versioncheck",
		"filename": "primepostrevisions.json",
		"ssl": true
	}
}
