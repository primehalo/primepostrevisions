{
	"name": "primehalo/primepostrevisions",
	"type": "phpbb-extension",
	"description": "Keeps a revision history of all post edits. These revisions can be viewed, deleted, and restored only by users with the necessary permissions.",
	"homepage": "https://github.com/primehalo/primepostrevisions",
	"version": "1.0.5",
	"time": "2022-07-16",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Ken F. Innes IV",
			"email": "primehalo@gmail.com",
			"homepage": "https://www.absoluteanime.com/",
			"role": "Lead Developer"
		},
		{
			"name": "Dark❶",
			"email": "phpbb@dark1.tech",
			"homepage": "https://dark1.tech",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.4.7"
	},
	"extra": {
		"display-name": "Prime Post Revisions",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3,<3.4.0@dev"
		},
		"version-check": {
			"host": "www.absoluteanime.com",
			"directory": "/_phpbb-extensions_/versioncheck",
			"filename": "primepostrevisions.json",
			"ssl": true
		}
	}
}
